<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
  <group id="replication">
    <!-- After Omni Replication-->
    <job name="repl_attributes" instance="Ls\Replication\Cron\AttributesCreateTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_category" instance="Ls\Replication\Cron\CategoryCreateTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_products" instance="Ls\Replication\Cron\ProductCreateTask" method="execute">
      <schedule>*/10 * * * *</schedule>
    </job>
    <job name="repl_barcode_update" instance="Ls\Replication\Cron\BarcodeUpdateTask" method="execute">
      <schedule>*/12 * * * *</schedule>
    </job>
    <job name="repl_discount_create" instance="Ls\Replication\Cron\DiscountCreateTask" method="execute">
      <schedule>*/20 * * * *</schedule>
    </job>
  </group>
  <group id="flat_replication">
    <!-- Most Important-->
    <job name="repl_item" instance="Ls\Replication\Cron\ReplEcommItemTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_barcode" instance="Ls\Replication\Cron\ReplEcommBarcodeTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_extended_variant_value" instance="Ls\Replication\Cron\ReplEcommExtendedVariantValueTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_image_link" instance="Ls\Replication\Cron\ReplEcommImageLinkTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_item_variant_registration" instance="Ls\Replication\Cron\ReplEcommItemVariantRegistrationTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_hierarchy" instance="Ls\Replication\Cron\ReplEcommHierarchyTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_hierarchy_node" instance="Ls\Replication\Cron\ReplEcommHierarchyNodeTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_hierarchy_leaf" instance="Ls\Replication\Cron\ReplEcommHierarchyLeafTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_attribute" instance="Ls\Replication\Cron\ReplEcommAttributeTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_attribute_value" instance="Ls\Replication\Cron\ReplEcommAttributeValueTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_attribute_option_value" instance="Ls\Replication\Cron\ReplEcommAttributeOptionValueTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_customer" instance="Ls\Replication\Cron\ReplEcommCustomerTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_discount" instance="Ls\Replication\Cron\ReplEcommDiscountTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <!-- Less Important-->
    <job name="repl_currency" instance="Ls\Replication\Cron\ReplEcommCurrencyTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_currency_exch_rate" instance="Ls\Replication\Cron\ReplEcommCurrencyExchRateTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_item_category" instance="Ls\Replication\Cron\ReplEcommItemCategoryTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_item_unit_of_measure" instance="Ls\Replication\Cron\ReplEcommItemUnitOfMeasureTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_price" instance="Ls\Replication\Cron\ReplEcommPriceTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_product_group" instance="Ls\Replication\Cron\ReplEcommProductGroupTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_store" instance="Ls\Replication\Cron\ReplEcommStoreTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_unit_of_measure" instance="Ls\Replication\Cron\ReplEcommUnitOfMeasureTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_vendor" instance="Ls\Replication\Cron\ReplEcommVendorTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_loy_vendor_item_mapping" instance="Ls\Replication\Cron\ReplEcommLoyVendorItemMappingTask"
         method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_data_translation" instance="Ls\Replication\Cron\ReplEcommDataTranslationTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_discount_validation" instance="Ls\Replication\Cron\ReplEcommDiscountValidationTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_shipping_agent" instance="Ls\Replication\Cron\ReplEcommShippingAgentTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_country_code" instance="Ls\Replication\Cron\ReplEcommCountryCodeTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_store_tender_type" instance="Ls\Replication\Cron\ReplEcommStoreTenderTypeTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
  </group>
</config>
