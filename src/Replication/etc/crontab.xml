<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
  <group id="replication">
    <!-- After Omni Replication-->
    <job name="repl_attributes" instance="Ls\Replication\Cron\AttributesCreateTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_category" instance="Ls\Replication\Cron\CategoryCreateTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_products" instance="Ls\Replication\Cron\ProductCreateTask" method="execute">
      <schedule>*/10 * * * *</schedule>
    </job>
    <job name="repl_barcode_update" instance="Ls\Replication\Cron\BarcodeUpdateTask" method="execute">
      <schedule>*/12 * * * *</schedule>
    </job>
    <job name="repl_discount_create" instance="Ls\Replication\Cron\DiscountCreateTask" method="execute">
      <schedule>*/20 * * * *</schedule>
    </job>
    <!-- Most Important-->
    <job name="repl_item" instance="Ls\Replication\Cron\ReplItemTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_barcode" instance="Ls\Replication\Cron\ReplBarcodeTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_extended_variant_value" instance="Ls\Replication\Cron\ReplExtendedVariantValueTask"
         method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_image_link" instance="Ls\Replication\Cron\ReplImageLinkTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_item_variant_registration" instance="Ls\Replication\Cron\ReplItemVariantRegistrationTask"
         method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_hierarchy" instance="Ls\Replication\Cron\ReplHierarchyTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_hierarchy_node" instance="Ls\Replication\Cron\ReplHierarchyNodeTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_hierarchy_leaf" instance="Ls\Replication\Cron\ReplHierarchyLeafTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_attribute" instance="Ls\Replication\Cron\ReplAttributeTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_attribute_value" instance="Ls\Replication\Cron\ReplAttributeValueTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_attribute_option_value" instance="Ls\Replication\Cron\ReplAttributeOptionValueTask"
         method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <job name="repl_customer" instance="Ls\Replication\Cron\ReplCustomerTask" method="execute">
      <schedule>*/2 * * * *</schedule>
    </job>
    <!-- Less Important-->
    <job name="repl_currency" instance="Ls\Replication\Cron\ReplCurrencyTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_currency_exch_rate" instance="Ls\Replication\Cron\ReplCurrencyExchRateTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_image" instance="Ls\Replication\Cron\ReplImageTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_item_category" instance="Ls\Replication\Cron\ReplItemCategoryTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_item_unit_of_measure" instance="Ls\Replication\Cron\ReplItemUnitOfMeasureTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_price" instance="Ls\Replication\Cron\ReplPriceTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_product_group" instance="Ls\Replication\Cron\ReplProductGroupTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_store" instance="Ls\Replication\Cron\ReplStoreTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_unit_of_measure" instance="Ls\Replication\Cron\ReplUnitOfMeasureTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_vendor" instance="Ls\Replication\Cron\ReplVendorTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_loy_vendor_item_mapping" instance="Ls\Replication\Cron\ReplLoyVendorItemMappingTask"
         method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_data_translation" instance="Ls\Replication\Cron\ReplDataTranslationTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="loy_item" instance="Ls\Replication\Cron\LoyItemTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_discount" instance="Ls\Replication\Cron\ReplDiscountTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_discount_validation" instance="Ls\Replication\Cron\ReplDiscountValidationTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_shipping_agent" instance="Ls\Replication\Cron\ReplShippingAgentTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_country_code" instance="Ls\Replication\Cron\ReplCountryCodeTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
    <job name="repl_store_tender_type" instance="Ls\Replication\Cron\ReplStoreTenderTypeTask" method="execute">
      <schedule>*/5 * * * *</schedule>
    </job>
  </group>
</config>
